{
  "name": "find-alias",
  "version": "0.0.0",
  "description": "interactive aliases finder and executor",
  "main": "src/index.js",
  "bin": {
    "fa": "src/index.js"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "mocha --recursive \"./test/unit/*.spec.js\"",
    "test:e2e": "mocha --recursive \"./test/e2e/*.spec.js\"",
    "test:watch": "mocha --parallel --watch --recursive \"./test/**/*.spec.js\"",
    "lint:check": "eslint . && prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [
    "alias"
  ],
  "author": "blzsaa",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "chai": "^4.3.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "sinon": "^13.0.2",
    "strip-ansi": "^7.0.1",
    "tmp": "^0.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "fuse.js": "^6.5.3",
    "inquirer": "^8.2.2",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "inquirer-test": "^2.0.1",
    "yargs": "^17.4.1"
  }
}
