{
  "name": "find-alias",
  "version": "0.1.0-0",
  "description": "interactive aliases finder and executor",
  "main": "dist/index.js",
  "bin": {
    "fa": "dist/index.js"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -b && tsc-alias -p tsconfig.json",
    "postbuild": "chmod +x dist/index.js",
    "prepublishOnly": "npm run build",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "ts-mocha --paths './test/unit/*.spec.ts'",
    "test:unit:watch": "ts-mocha --paths './test/unit/**/*.spec.ts' -w --watch-files '**/*.ts'",
    "test:e2e": "npm run build && ts-mocha --paths './test/e2e/*.spec.ts'",
    "lint:check": "eslint . && prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "prepare": "husky install",
    "release": "release-it"
  },
  "dependencies": {
    "fuse.js": "^6.5.3",
    "inquirer": "^8.2.2",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "inquirer-test": "^2.0.1",
    "picocolors": "^1.0.0",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/chai": "^4.3.1",
    "@types/inquirer": "^8.2.1",
    "@types/inquirer-autocomplete-prompt": "^1.3.4",
    "@types/mocha": "^9.1.1",
    "@types/sinon": "^10.0.11",
    "@types/tmp": "^0.2.3",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "chai": "^4.3.6",
    "chai-string": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.0",
    "prettier": "^2.6.2",
    "release-it": "^15.0.0",
    "rimraf": "^3.0.2",
    "tmp": "^0.2.1",
    "ts-mocha": "^10.0.0",
    "ts-sinon": "^2.0.2",
    "tsc-alias": "^1.6.7",
    "typescript": "^4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "alias",
    "bash",
    "zsh",
    "console",
    "terminal",
    "command",
    "commands",
    "finder",
    "executor"
  ],
  "author": "blzsaa",
  "license": "MIT",
  "homepage": "https://github.com/blzsaa/find-alias#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/blzsaa/find-alias.git"
  },
  "bugs": "https://github.com/blzsaa/find-alias/issues",
  "files": [
    "dist/**/*.js",
    "find-alias.sh"
  ]
}
